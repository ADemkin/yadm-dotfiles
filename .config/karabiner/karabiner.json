{
    "global": {
        "check_for_updates_on_startup": false,
        "show_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.to_delayed_action_delay_milliseconds": 300,
                    "basic.to_if_alone_timeout_milliseconds": 500,
                    "basic.to_if_held_down_threshold_milliseconds": 150
                },
                "rules": [
                    {
                        "description": "Fn + f5/f6 to keyboard brightness down/up",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f5",
                                    "modifiers": { "mandatory": ["fn"] }
                                },
                                "to": [{ "key_code": "illumination_decrement" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "f6",
                                    "modifiers": { "mandatory": ["fn"] }
                                },
                                "to": [{ "key_code": "illumination_increment" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Fn to Capclock if pressed alone",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "fn",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "fn",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left and right shifts pressed togerher to Capslock",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "right_shift",
                                    "modifiers": { "mandatory": ["left_shift"] }
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "left_shift",
                                    "modifiers": { "mandatory": ["right_shift"] }
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "simultaneous": [
                                        { "key_code": "left_shift" },
                                        { "key_code": "right_shift" }
                                    ]
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change caps_lock to left_control if pressed with other keys, change caps_lock to escape if pressed alone.",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "to_if_alone": [{ "key_code": "escape" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Swap Command and Ctrl keys for Alacritty.app",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^io\\.alacritty$",
                                            "^org\\.alacritty$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "left_control" },
                                "to": [{ "key_code": "left_command" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^io\\.alacritty$",
                                            "^org\\.alacritty$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "left_command" },
                                "to": [{ "key_code": "left_control" }],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 628,
                        "vendor_id": 1452
                    },
                    "manipulate_caps_lock_led": false
                },
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 254,
                        "vendor_id": 1539
                    },
                    "manipulate_caps_lock_led": false,
                    "simple_modifications": [
                        {
                            "from": { "key_code": "left_command" },
                            "to": [{ "key_code": "left_option" }]
                        },
                        {
                            "from": { "key_code": "left_control" },
                            "to": [{ "key_code": "left_command" }]
                        },
                        {
                            "from": { "key_code": "left_option" },
                            "to": [{ "key_code": "left_control" }]
                        }
                    ]
                },
                {
                    "identifiers": {
                        "is_keyboard": true,
